package com.rohit;

public class Watch {
	String name;
	int price;
	String manufacturer;
	
	public Watch(String name, int price, String manufacturer) {
		this.name = name;
		this.price = price;
		this.manufacturer = manufacturer;
	}
	
	public String getName() { return name; }
	public int getPrice() { return price; }
	public String getManufacturer() { return manufacturer; }
}
